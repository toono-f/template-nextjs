{
  "name": "study-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix:lint": "next lint --fix",
    "export": "next build && next export -o out",
    "serve": "yarn export -o out && serve ./out",
    "export-demo": "NEXT_PUBLIC_ENVIRONMENT=demo next build && next export -o out_demo",
    "export-all": "NEXT_PUBLIC_ENVIRONMENT=demo next build && next export -o out_demo && next build && next export -o out",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "fix:css": "stylelint --fix \"src/**/*.scss\"",
    "validate": "w3c-html-validator out/**/ out/*.html"
  },
  "dependencies": {
    "next": "12.0.4",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.33.1",
    "swiper": "^7.4.1",
    "swr": "^1.1.1",
    "tailwindcss": "^3.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.14",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-tailwindcss": "^3.4.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "responsive-loader": "^2.3.0",
    "sass": "^1.45.0",
    "serve": "^13.0.2",
    "sharp": "^0.29.3",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwind-config-viewer": "^1.6.3",
    "w3c-html-validator": "^1.0.1",
    "webp-loader": "^0.6.0"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
