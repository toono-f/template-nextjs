{
  "name": "study-app",
  "private": true,
  "scripts": {
    "webpack": "NODE_ENV=development webpack --watch",
    "webpack-build": "NODE_ENV=production webpack",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "fix:lint": "next lint --fix",
    "fix:css": "stylelint --fix \"src/**/*.scss\"",
    "validate": "w3c-html-validator out/**/ out/*.html",
    "export": "yarn webpack-build && next build && next export -o out && yarn validate",
    "export-dev": "yarn webpack-build && NEXT_PUBLIC_ENVIRONMENT=development next build && next export -o out_dev && yarn validate",
    "export-all": "yarn export && yarn export-dev",
    "export-gulp": "yarn export && NODE_ENV=production gulp",
    "export-gulp-dev": "yarn export-dev && NODE_ENV=development gulp",
    "export-gulp-all": "yarn export-gulp && yarn export-gulp-dev",
    "serve": "yarn export && serve ./out",
    "serve-gulp": "yarn export-gulp && serve ./out",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "rem": "yarn gulp remConversion"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "gsap": "^3.9.1",
    "next": "12.0.4",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.33.1",
    "swiper": "^8.0.3",
    "swr": "^1.1.1",
    "tailwindcss": "^3.0.7",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "cssnano": "^5.0.14",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-tailwindcss": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "responsive-loader": "^2.3.0",
    "sass": "^1.45.0",
    "serve": "^13.0.2",
    "sharp": "^0.29.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwind-config-viewer": "^1.6.3",
    "terser-webpack-plugin": "^5.3.1",
    "w3c-html-validator": "^1.0.1",
    "webp-loader": "^0.6.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
